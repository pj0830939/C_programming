#include<stdio.h>
#include<stdio.h>

int main()
{
  int phy ,chem , maths;
  int total;
  float percentage;
  
  
  printf("\n Enter marks of three subject:");
  scanf("%d %d %d",phy ,chem , maths );
  
  total = phy +chem +maths;
  percentage = (total/300.0)*100;
  
  printf("total marks = %d\n", total);
  printf("percentage = %.2f%%\n", percentage);
  
  getch();
  return 0;
}
